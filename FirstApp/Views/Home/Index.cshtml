@model IEnumerable<Employee>

@{
    ViewBag.Title = "All Emp";
}

<div class="card-deck">
    @foreach (var employee in Model)
        {
    <div class="card m-3">
        <div class="card-header">
            <h3>@employee.Name</h3>
        </div>
        <img src="~/test.png"  class="card-img-top" height="150" asp-append-version="true"/>
        <div class="card-footer text-center" >
            
         
            <a asp-controlle="Home" asp-action="details" asp-route-id="@employee.Id" class="btn btn-primary">View</a>
            <a href="#" class="btn btn-primary" >Edit</a>
            <a href="#" class="btn btn-danger" >Delete</a>
        </div>
    </div> 
        }
</div>

